{
    "name": "antmove",
    "version": "1.0.20",
    "description": "小程序转换器，基于支付宝/微信小程序转换为多端小程序，让小程序跨平台开发变得简单。",
    "main": "src/index.js",
    "bin": {
        "antmove": "./bin/index"
    },
    "scripts": {
        "build-iview": "antmove wx-alipay -i ./examples/view-weapp -o ./examples/wx-alipay/view-aliapp",
        "build-mini": "antmove wx-alipay -i ./examples/miniprogram-demo/miniprogram -o ./examples/miniprogram-demo/out",
        "test-suites": "npm run build-iview",
        "test": "jest",
        "link": "bash ./build/link.local.sh",
        "lint": "eslint ./src",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "amap-doc": "node ./build/utils/outputFils.js",
        "build-external": "node ./build/buildDoc/external",
        "build-alife": "node ./build/buildAlipay/index.js"
    },
    "author": "amap-appx@service.autonavi.com",
    "license": "GPL-3.0",
    "dependencies": {
        "@antmove/alipay-baidu": "^1.0.0",
        "@antmove/alipay-polyfill": "^1.0.0",
        "@antmove/alipay-wx": "^1.0.0",
        "@antmove/utils": "^1.0.6",
        "@antmove/wx-alipay": "^1.0.0",
        "@antmove/wx-baidu": "^1.0.0",
        "@antmove/wx-qq": "^1.0.0",
        "@antmove/wx-tt": "^1.0.0",
        "@antmove/wx-wx": "^1.0.0",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "chalk": "^2.4.2",
        "commander": "^2.19.0",
        "figlet": "^1.2.3",
        "fs-extra": "^8.1.0",
        "inquirer": "^7.0.0",
        "jest": "^24.9.0",
        "marked": "^0.7.0",
        "os": "^0.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^7.5.0",
        "eslint": "^5.15.1",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-node": "^7.0.0",
        "eslint-plugin-promise": "^4.0.0",
        "eslint-plugin-standard": "^4.0.0",
        "eslint-utils": "^1.4.1",
        "gulp": "^4.0.1",
        "husky": "^1.3.1",
        "jest": "^24.9.0",
        "lerna": "^3.13.3",
        "recursive-readdir": "^2.2.2"
    },
    "keywords": [
        "miniprogram",
        "wechat-mini-program",
        "alipay-programm",
        "miniprogram-frame",
        "tool",
        "components",
        "baidu-miniprogram"
    ],
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "pre-commit": "npm run lint",
            "pre-push": "npm run test-suites && npm run test"
        }
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ant-move/antmove"
    },
    "sctipts": {
        "test": "jest"
    }
}